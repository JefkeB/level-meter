{
  "configurations": [
      {
          "name": "gcc",
          "includePath": [
              "${workspaceFolder}/source",
              "${workspaceFolder}/source",
              "${workspaceFolder}/CMSIS/Include",
              "${workspaceFolder}/CMSIS/Device/Include"
          ],
          "defines": [
              "METER50",
              "DEBUG"
          ],
          "compilerPath": "${env:GNUARM}",
          "cStandard": "c11",
          "cppStandard": "c++17",
          "intelliSenseMode": "gcc-x64",
          "browse": {
              "path": [
                  "${workspaceFolder}",
                  "${env:GNUARM}"
              ],
              "limitSymbolsToIncludedHeaders": true,
              "databaseFilename": ""
          }
      }
  ],
  "version": 4
}